    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="crÃ¨che balancelle moirans" />
    <script type="application/x-javascript"> addEventListener(
        "load", 
        function() { setTimeout(hideURLbar, 0); }, 
        false);
        function hideURLbar(){ window.scrollTo(0,1); } 
    </script>
    <!-- //custom-theme -->
    {% block stylesheets %}
        <link 
            rel="stylesheet" 
            href="{{ asset('bundles/app/css/bootstrap.css') }}" 
            type="text/css" 
            media="screen" 
        />
        <link 
            rel="stylesheet" 
            href="{{ asset('bundles/app/css/style.css') }}" 
            type="text/css" 
            media="screen" 
        />
        <link 
            rel="stylesheet" 
            href="{{ asset('bundles/app/css/easy-responsive-tabs.css') }}" 
            type="text/css" 
            media="screen" 
        />
        <link 
            rel="stylesheet" 
            href="{{ asset('bundles/app/css/jquery.gallery.css') }}" 
            type="text/css" 
            media="screen" 
        />
        <link 
            rel="stylesheet" 
            href="{{ asset('bundles/app/css/popuo-box.css') }}" 
            type="text/css" 
            media="screen" 
        />
        <!-- font-awesome-icons -->
